@media (max-width: 600px) {
  .wrapper { padding-left: 15px; padding-right: 15px; }
  .content-push { margin-top: 30px; }
  .post { margin-bottom: 15px; }
  .post .post-name { font-size: 32px; font-weight: 500; }
  .feedback { margin-top: 15px; }
  .pagination { text-align: center; }
  .pagination-archive { float: none; white-space: nowrap; margin: 0px 15px; }
  .archive ul { display: table; }
  .archive li { display: table-row; }
  .archive li > time, .archive li > a { display: table-cell; }
  .archive h3 { text-align: left; width: auto; }
  .archive time { width: auto; white-space: nowrap; padding-right: 15px; }
}

@media (max-width: 700px) {
  .top-bar { height: auto; padding: 15px 10px; }
  .top-bar > .wrapper { width: 100%; text-align: center; padding-left: 5px; padding-right: 5px; }
  .top-bar .navigation { vertical-align: middle; padding-bottom: 0px; }
  .top-bar .top-bar-right { display: none; }
  .footer { height: auto; padding: 20px 10px; }
  .footer > .wrapper { width: 100%; text-align: center; }
  .footer .footer-left, .footer .footer-right { float: none; }
  .footer .footer-right { margin-top: 15px; }
}
